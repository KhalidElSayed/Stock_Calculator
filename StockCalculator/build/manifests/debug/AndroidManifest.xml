<manifest xmlns:android="http://schemas.android.com/apk/res/android" android:versionCode="1" android:versionName="1.0" package="com.aman.stockcalculator">

    <original-package android:name="com.aman.stockcalculator"/>

    <uses-sdk android:minSdkVersion="10" android:targetSdkVersion="17"/>

    <uses-library android:name="com.google.common.collect.ImmutableMap"/>

    <application android:icon="@mipmap/ic_launcher_calculator" android:label="@string/app_name">
        <activity android:name="com.aman.stockcalculator.Calculator" android:theme="@android:style/Theme.Holo.NoActionBar" android:windowSoftInputMode="stateAlwaysHidden">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.DEFAULT"/>
                <category android:name="android.intent.category.LAUNCHER"/>
                <category android:name="android.intent.category.APP_CALCULATOR"/>
            </intent-filter>
        </activity>
    </application>
</manifest>
